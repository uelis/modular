let step =
   \f -> \ x ->
     if0 x then 1 else
       let x1 = intadd(x, -1) in
       if0 x1 then 1 else
         let x2 = intadd(x1, -1) in
         intadd(f x1, f x2)
let fib1 = fix f x -> step f x         
